{
  "name": "quasar-datepicker",
  "version": "1.0.0",
  "description": "Datepicker ui component based on quasar-ui-qcalendar",
  "private": false,
  "type": "module",
  "main": "./dist/quasar-datepicker.umd.cjs",
  "module": "./dist/quasar-datepicker.js",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/quasar-datepicker.js",
      "require": "./dist/quasar-datepicker.umd.cjs"
    },
    "./style" : "./dist/style.css",
    "./qdate": "./src/plugins/date.ts"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "peerDependencies": {
    "@quasar/quasar-ui-qcalendar": "^4.0.0-beta.16",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.70.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NamgyungKim/quasar-datepicker"
  },
  "keywords": [
    "vuejs",
    "vue",
    "quasar-ui-qcalendar",
    "datepicker",
    "dateselecter",
    "spa"
  ],
  "author": {
    "name": "Namgung Kim",
    "email": "namgyung.kim@gmail.com"
  },
  "license": "MIT"
}
